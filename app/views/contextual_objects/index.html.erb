<p id="notice"><%= notice %></p>

<h1>Contextual Objects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contextual_objects.each do |contextual_object| %>
      <tr>
        <td><%= link_to 'Show', contextual_object %></td>
        <td><%= link_to 'Edit', edit_contextual_object_path(contextual_object) %></td>
        <td><%= link_to 'Destroy', contextual_object, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contextual Object', new_contextual_object_path %>
