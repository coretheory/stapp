<% content_for :primary_content do %>

	<% if @project.header_image.attached? %>
    <%= image_tag @project.header_image %>
  <% end %>

<% end %>

<% content_for :secondary_content do %>

  <% if @project.footer_image.attached? %>
    <%= image_tag @project.footer_image %>
  <% end %>

<% end %>

<% if @project.one_grid_layout? %>
  <%= render "one_grid_layout" %>
<% elsif @project.two_grid_layout? %>
  <%= render "two_grid_layout" %>
<% elsif @project.three_grid_layout? %>
  <%= render "three_grid_layout" %>
<% elsif @project.alt_grid_layout? %>
  <%= render "alt_grid_layout" %>
<% elsif @project.thesis_grid_layout? %>
  <%= render "thesis_grid_layout" %>
<% else %>
  <%= render "basic_layout" %>
<% end %>